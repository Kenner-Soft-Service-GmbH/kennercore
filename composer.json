{
  "name": "treolabs/treocore",
  "version": "3.16.1",
  "description": "TreoCORE. Open source application.",
  "homepage": "https://treopim.com",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Roman Ratsun",
      "email": "r.ratsun@treolabs.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-pdo_mysql": "*",
    "ext-openssl": "*",
    "ext-json": "*",
    "ext-zip": "*",
    "ext-gd": "*",
    "ext-mbstring": "*",
    "ext-xml": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "wikimedia/composer-merge-plugin": "1.4.1",
    "treolabs/espocore": "^1.5.3"
  },
  "minimum-stability": "stable",
  "extra": {
    "treo-source": "https://source.treopim.com",
    "merge-plugin": {
      "include": [
        "data/composer.json",
        "data/repositories.json",
        "data/dev-composer.json"
      ]
    }
  },
  "autoload": {
    "psr-0": {
      "": "application/",
      "Espo\\Custom": "custom/"
    },
    "psr-4": {
      "Treo\\Migrations\\": "data/migrations/"
    }
  },
  "scripts": {
    "post-install-cmd": "Treo\\Composer\\Cmd::postInstall",
    "pre-update-cmd": "Treo\\Composer\\Cmd::preUpdate",
    "post-update-cmd": "Treo\\Composer\\Cmd::postUpdate"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.treopim.com/packages.json?id=public"
    }
  ]
}
